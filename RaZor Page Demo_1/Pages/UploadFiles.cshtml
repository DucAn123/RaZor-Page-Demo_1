@page
@model RazorPagesLabA1.Pages.UploadFilesModel

<form method="post" enctype="multipart/form-data">
    <label asp-for="FileUploads"></label>
    <input asp-for="FileUploads" />
    <span asp-validation-for="FileUploads" class="text-danger"></span>
    <br />
    <button class="btn btn-primary" asp-page-handler="UploadMulti">Upload</button>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
